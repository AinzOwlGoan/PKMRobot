function [AutoVal,r] = Manipolabilita(Jmat)
%MANIPOLABILITA Summary of this function goes here
%   Detailed explanation goes here
k = 1;
for i=1:length(Jmat)/2
    
    J = Jmat(:,k:k+1);
    
    [AutoVal,AutoVett] = eig(J*J');
    
    r(1,i) = max(AutoVal)/min(AutoVal);
    
    k = k+1;
end
end

